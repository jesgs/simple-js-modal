{
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true,
            "impliedStrict": true
        }
    },
    "env": {
        "browser": true
    },
    "rules": {
        "semi": 2,
        "quotes": ["error", "single"],
        "no-alert": "warn",
        "no-console": "warn",
        "no-var": "error",
        "no-mixed-spaces-and-tabs": "warn",
        "no-trailing-spaces": "warn",
        "camelcase": "error"
    }
}